{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "habchi.ouafid@gmail.com"
      }
    }
  }
}
